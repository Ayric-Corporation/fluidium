        <parameter type="text" name="to element with id" code="IDen" optional="yes" description="The HTML id attribute of the element upon which the event will be dispatched.">
            <cocoa key="identifier"/>
        </parameter>
        <parameter type="text" name="to element with name" code="Name" optional="yes" description="The HTML name attribute of the element upon which the event will be dispatched.">
            <cocoa key="name"/>
        </parameter>
        <parameter type="text" name="to element for XPath" code="XPth" optional="yes" description="The XPath for the element upon which the event will be dispatched.">
            <cocoa key="xpath"/>
        </parameter>
        <parameter type="text" name="to element for CSS selector" code="CSSs" optional="yes" description="The CSS selector for the element upon which the event will be dispatched.">
            <cocoa key="cssSelector"/>
        </parameter>

        <parameter type="text" name="related target with id" code="RtID" optional="yes" description="The HTML id attribute of the related target of the event.">
            <cocoa key="relatedIdentifier"/>
        </parameter>
        <parameter type="text" name="related target with name" code="RtNa" optional="yes" description="The HTML name attribute of the related target of the event.">
            <cocoa key="relatedName"/>
        </parameter>
        <parameter type="text" name="related target for XPath" code="RtXP" optional="yes" description="The XPath for the related target of the event.">
            <cocoa key="relatedXPath"/>
        </parameter>
        <parameter type="text" name="related target for CSS selector" code="RtCS" optional="yes" description="The CSS selector for the related target of the event.">
            <cocoa key="relatedCSSSelector"/>
        </parameter>

        <parameter type="integer" name="click count" code="ClCt" optional="yes" description="The number of clicks. Send 2 for a double click.">
            <cocoa key="clickCount"/>
        </parameter>
        <parameter type="integer" name="button" code="Butn" optional="yes" description="Send 0 for left button (default), 1 for middle button, 2 for right button.">
            <cocoa key="button"/>
        </parameter>
        <parameter type="boolean" name="control key pressed" code="Ctrl" optional="yes" description="The control key is pressed during mouse event.">
            <cocoa key="ctrlKeyPressed"/>
        </parameter>
        <parameter type="boolean" name="option key pressed" code="Alt " optional="yes" description="The option key is pressed during mouse event.">
            <cocoa key="altKeyPressed"/>
        </parameter>
        <parameter type="boolean" name="shift key pressed" code="Shft" optional="yes" description="The shift key is pressed during mouse event.">
            <cocoa key="shiftKeyPressed"/>
        </parameter>
        <parameter type="boolean" name="command key pressed" code="Meta" optional="yes" description="The command key is pressed during mouse event.">
            <cocoa key="metaKeyPressed"/>
        </parameter>
        <parameter type="tab" name="in" code="tPrm" optional="yes" description="A tab in which to dispatch the event. If not specified, the selected tab in the frontmost window is used.">
            <cocoa key="tabController"/>
        </parameter>
    </command>

