<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Browser -->
    <key>FURecentURLs</key>
    <array/>
    <key>FUMatchingURLPatterns</key>
    <array/>

    <!-- WebKit -->
    <!-- <key>WebIconDatabaseDirectoryDefaults</key>
    <string>~/Library/Application Support/Fluidium/IconDatabase</string> -->
    <key>WebKitDeveloperExtras</key>
    <true/>
    <key>FUContinuousSpellCheckingEnabled</key>
    <true/>
    <key>FUZoomTextOnly</key>
    <false/>

    <!-- UI -->
    <key>FUBookmarkBarShown</key>
    <false/>
    <key>FUBookmarkBarShowsFavicons</key>
    <false/>
    <key>FUStatusBarShown</key>
    <true/>
    <key>FUTabBarHiddenAlways</key>
    <false/>
    <key>FUWindowFrameString</key>
	<string>{{130, 50}, {1020, 700}}</string>
    <key>FUWindowScreenIndex</key>
    <integer>0</integer>
    <key>FUPlugInDrawerContentSizeString</key>
	<string>{320, 160}</string>

    <!-- General -->
    <key>FUNewWindowsOpenWith</key>
    <integer>0</integer>
    <key>FUHomeURLString</key>
    <string>http://google.com</string>
    <key>FUDownloadDirPath</key>
    <string>~/Downloads</string>
    
    <!-- Appearance -->
    <key>FULoadsImagesAutomatically</key>
    <integer>1</integer>

    <!-- Behavior -->
    <key>FUSpacesBehavior</key>
    <integer>0</integer>
    <key>FUTargetedClicksCreateTabs</key>
    <true/>
    <key>FULinksSentToOtherApplicationsOpenInBackground</key>
    <false/>
    <key>FUOpenLinksFromApplicationsIn</key>
    <integer>0</integer>
    <key>FUHideLastClosedWindow</key>
    <false/>
    <key>FUSessionsEnabled</key>
    <true/>

    <!-- Tabs -->
    <key>FUTabbedBrowsingEnabled</key>
    <true/>
    <key>FUSelectNewWindowsOrTabsAsCreated</key>
    <true/>
    <key>FUTabBarHiddenForSingleTab</key>
    <true/>
    <key>FUNewTabsOpenInline</key>
    <true/>
    <key>FUSelectPreviouslySelectedTabOnTabClose</key>
    <true/>
    <key>FUTabBarCellOptimumWidth</key>
    <integer>175</integer>
    <key>FUConfirmBeforeClosingMultipleTabsOrWindows</key>
    <true/>
    
    <!-- Security -->
    <key>FUPlugInsEnabled</key>
    <true/>
    <key>FUJavaEnabled</key>
    <true/>
    <key>FUJavaScriptEnabled</key>
    <true/>
    <key>FUJavaScriptCanOpenWindowsAutomatically</key>
    <false/>
    <key>FUCookieAcceptPolicy</key>
    <integer>2</integer>
    
    <!-- Advanced -->
    <key>FUAllowBrowsingToAnyDomain</key>
    <true/>
    <key>FUInvertWhitelist</key>
    <false/>
    
    <!-- PlugIns -->
    <key>FUNumberOfBrowsaPlugIns</key>
    <integer>2</integer>
    
</dict>
</plist>
